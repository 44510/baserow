{
  "name": "Baserow",
  "description": "Open source online no-code database and Airtable alternative.",
  "repository": "https://gitlab.com/bramw/baserow",
  "keywords": ["baserow", "no-code", "database", "airtable"],
  "addons": [
    "heroku-postgresql:hobby-dev",
    "heroku-redis:hobby-dev",
    "mailgun:starter"
  ],
  "env": {
    "SECRET_KEY": {
      "description": "@TODO",
      "generator": "secret"
    },
    "BASEROW_PUBLIC_URL": "%(HEROKU_APP_NAME)s",
    "BASEROW_PUBLIC_URL_2": "${HEROKU_APP_NAME}",
    "BASEROW_PUBLIC_URL_3": "$(HEROKU_APP_NAME)",
    "BASEROW_PUBLIC_URL_4": "$(HEROKU_APP_NAME)s"
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "labs": [
    "runtime-dyno-metadata"
  ],
  "stack": "container"
}
